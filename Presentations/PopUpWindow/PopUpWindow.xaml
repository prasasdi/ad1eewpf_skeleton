<Window x:Class="BelajarNotifyPropertyChange.Presentations.PopUpWindow.PopUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BelajarNotifyPropertyChange.Presentations.PopUpWindow"
        mc:Ignorable="d"
        Title="PopUpWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tb_name" HorizontalAlignment="Left" Margin="79,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"
                 Text="{Binding Person.Name, Mode=OneWay}" />

    </Grid>
</Window>
